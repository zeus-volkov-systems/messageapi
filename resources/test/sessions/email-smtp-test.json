{
    "plugin": "gov.noaa.messageapi.sessions.PublisherSession",
    "constructor": {
        "schema": {
            "plugin": "gov.noaa.messageapi.schemas.DefaultSchema",
            "constructor": {
                "metadata": "{}/resources/test/schemas/simple/email/metadata.json",
                "fields": "{}/resources/test/schemas/simple/email/fields.json",
                "conditions": "{}/resources/test/schemas/simple/email/conditions.json",
                "operators": "gov.noaa.messageapi.factories.OperatorFactory"
            }
        },
        "container": {
            "plugin": "gov.noaa.messageapi.containers.DefaultContainer",
            "constructor": {
                "metadata": "{}/resources/test/containers/simple/email/metadata.json",
                "bins": "{}/resources/test/containers/simple/email/bins.json",
                "relationships": "{}/resources/test/containers/simple/email/relationships.json"
            }
        },
        "protocol": {
            "plugin": "gov.noaa.messageapi.protocols.DefaultProtocol",
            "constructor": {
                "metadata": "{}/resources/test/protocols/email/simple/metadata.json",
